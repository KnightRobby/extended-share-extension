{
  "name": "Extended Share for Google Plus",
  "version": "1.2.0",
  "description": "Extends Google+ to share to Facebook and Twitter.",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": ["tabs", "https://plus.google.com/*", "http://plus.google.com/*"],
  "background_page": "background.html",
  "content_scripts": [{
      "matches": ["https://plus.google.com/*", "http://plus.google.com/*"],
      "js": ["js/injection.js"],
      "run_at": "document_end",
      "all_frames": true
  }]
}
